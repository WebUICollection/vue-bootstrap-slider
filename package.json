{
  "name": "vue-bootstrap-slider",
  "version": "1.3.2",
  "description": "Vue bindings for seiyria/bootstrap-slider",
  "author": "pimlie <pimlie@hotmail.com>",
  "main": "dist/vue-bootstrap-slider.common.js",
  "web": "dist/vue-bootstrap-slider.js",
  "module": "dist/vue-bootstrap-slider.esm.js",
  "jsnext:main": "dist/vue-bootstrap-slider.esm.js",
  "license": "MIT",
  "homepage": "https://github.com/pimlie/vue-bootstrap-slider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pimlie/vue-bootstrap-slider.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c build/rollup.config.js",
    "lint": "eslint --ignore-path .gitignore --ext .js,.vue lib",
    "test": "jest"
  },
  "keywords": [
    "vue-component",
    "vue",
    "component",
    "bootstrap",
    "bootstrap-slider"
  ],
  "dependencies": {
    "bootstrap-slider": "^10.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.1",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "lodash": "^4.17.10",
    "rollup": "^0.66.2",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^6.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "rollup-watch": "^4.0.0",
    "uglify-es": "^3.3.10",
    "vue": "^2.5.16",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.17"
  },
  "jest": {
    "testRegex": "spec.js$",
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest",
      ".*\\.(css)$": "jest-css-modules"
    }
  }
}
